{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style" : ["error", "windows"],
        "no-restricted-syntax": [
            "error",
            "windows",
            "LabeledStatement",
            "WithStatement"
        ]
    }
}
